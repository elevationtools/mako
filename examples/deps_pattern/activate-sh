
activate_example_deps_pattern() {
  prev_dir=""
  this_dir="$PWD"
  while ! test -e "$this_dir/activate-sh"; do
    this_dir="$(dirname "$this_dir")"
    test "$this_dir" != "$prev_dir" || {
      echo "Couldn't find repo root"
      return 1
    } >&2
  done

  export PATH=$PWD/deps/bin:$PATH
}

activate_example_deps_pattern

